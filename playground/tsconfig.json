{
  "compilerOptions": {
    "outDir": "dist",
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "strict": true,
    "declaration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "preserveSymlinks": true
  },
  "include": [
      "@types/*.d.ts",
      ".aeria/aeria.d.ts",
      "src"
  ]
}
