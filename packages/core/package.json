{
  "name": "@aeriajs/core",
  "version": "0.0.64",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "TS_NODE_COMPILER_OPTIONS=\"$(cat ../compilerOptions.json)\" mocha -r ts-node/register tests/*.spec.ts",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "build": "pnpm build:cjs && pnpm build:esm",
    "build:cjs": "tsc",
    "build:esm": "esbuild './src/**/*.ts' --outdir=dist --out-extension:.js=.mjs && pnpm build:esm-transform",
    "build:esm-transform": "pnpm -w esm-transform $PWD/dist",
    "prepublishOnly": "pnpm build",
    "postinstall": "node dist/__scripts__/postinstall.js || true"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./presets": {
      "import": "./dist/presets/index.mjs",
      "require": "./dist/presets/index.js",
      "types": "./dist/presets/index.d.ts"
    },
    "./presets/*": "./dist/presets/*"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@aeriajs/access-control": "^0.0.35",
    "@aeriajs/builtins": "^0.0.64",
    "@aeriajs/common": "^0.0.35",
    "@aeriajs/entrypoint": "^0.0.35",
    "@aeriajs/http": "^0.0.43",
    "@aeriajs/security": "^0.0.64",
    "@aeriajs/types": "^0.0.32",
    "@aeriajs/validation": "^0.0.38",
    "@types/jsonwebtoken": "^9.0.6",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^6.1.0",
    "mongodb-memory-server": "^9.1.7"
  },
  "peerDependencies": {
    "@aeriajs/access-control": "^0.0.35",
    "@aeriajs/builtins": "^0.0.64",
    "@aeriajs/common": "^0.0.35",
    "@aeriajs/entrypoint": "^0.0.35",
    "@aeriajs/http": "^0.0.43",
    "@aeriajs/security": "^0.0.64",
    "@aeriajs/types": "^0.0.32",
    "@aeriajs/validation": "^0.0.38",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^6.1.0"
  },
  "optionalDependencies": {
    "mongodb-memory-server": "^9.1.7"
  }
}
